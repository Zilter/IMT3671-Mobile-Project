package com.project.gemswapper;

import android.graphics.Bitmap;
import android.graphics.Canvas;
import android.graphics.Rect;

public class Tile {
	byte mType;
	Bitmap mSprite;
	
	private Rect mSize;
	
	
	public void draw(Canvas canvas)
	{
		Rect dest = new Rect(getXPos(), getYPos(), getXPos + spriteWidth, getYPos() + spirteHeight);
		
		canvas.drawBitmap(mSprite, mSize, dest, null);
	}
}
